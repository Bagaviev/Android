<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.example.reclinetask">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/reklain"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/reklain"
            android:supportsRtl="true"
            android:theme="@style/Theme.ReclineTask">
        <activity android:name=".LoadActivity"></activity>
        <activity
                android:name=".WebViewActivity"
                android:theme="@style/Theme.ReclineTask.NoActionBar"></activity>
        <activity
                android:name=".MainActivity"
                android:screenOrientation="portrait"
                android:theme="@style/Theme.ReclineTask.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>